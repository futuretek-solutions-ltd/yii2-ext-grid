{
    "name": "futuretek/yii2-grid",
    "description": "Custom Futuretek Grid",
    "keywords": [
        "yii2",
        "extension",
        "grid"
    ],
    "type": "yii2-extension",
    "license": "proprietary",
    "homepage": "http://www.futuretek.cz/",
    "authors": [
        {
            "name": "Petr Leo Compel",
            "email": "petr.compel@futuretek.cz"
        }
    ],
    "support": {
        "issues": "https://jira.futuretek.cz/browse/YII",
        "wiki": "https://doc.futuretek.cz/display/YII",
        "source": "https://git.futuretek.cz/projects/YII/repos/ext-grid/browse"
    },
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "^2.0.0",
        "kartik-v/yii2-grid": "~3.1",
        "himiklab/yii2-sortable-grid-view-widget": "1.0.*"
    },
    "repositories": [
        {
            "url": "https://composer.futuretek.cz/repo/private/",
            "type": "composer"
        }
    ],
    "autoload": {
        "psr-4": {
            "futuretek\\grid\\": ""
        }
    },
    "extra": {
        "yii-config": {
            "modules": {
                "gridview": {
                    "class": "\\kartik\\grid\\Module"
                }
            }
        }
    }
}

{
    "name": "futuretek/yii2-grid",
    "description": "Custom Futuretek Grid",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","grid"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Petr Leo Compel",
            "email": "petr.compel@futuretek.cz"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "kartik-v/yii2-grid": "*"
    },
    "autoload": {
        "psr-4": {
            "futuretek\\grid\\": ""
        }
    },
    "extra": {
        "yii-config": {
            "modules": {
                "gridview": {
                    "class": "\\kartik\\grid\\Module"
                }
            }
        }
    }
}

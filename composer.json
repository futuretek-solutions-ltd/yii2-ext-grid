{
    "name": "futuretek/yii2-grid",
    "description": "Custom Futuretek Grid",
    "keywords": ["yii2","extension","grid"],
    "type": "yii2-extension",
    "license": "proprietary",
    "homepage": "http://www.futuretek.cz/",
    "authors": [
        {
            "name": "Petr Leo Compel",
            "email": "petr.compel@futuretek.cz"
        }
    ],
    "support": {
        "issues": "https://jira.futuretek.cz/browse/YII",
        "wiki": "https://doc.futuretek.cz/display/YII",
        "source": "https://git.futuretek.cz/projects/YII"
    },
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.*",
        "kartik-v/yii2-grid": "dev-master",
        "himiklab/yii2-sortable-grid-view-widget" : "1.0.*"
    },
    "repositories": [
        {
            "url": "http://composer.futuretek.cz/repo/private/",
            "type": "composer"
        }
    ],
    "autoload": {
        "psr-4": {
            "futuretek\\grid\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "yii-config": {
            "modules": {
                "gridview": {
                    "class": "\\kartik\\grid\\Module"
                }
            }
        }
    }
}

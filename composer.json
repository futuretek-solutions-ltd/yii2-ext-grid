{
    "name": "futuretek/yii2-grid",
    "description": "Custom Futuretek Grid",
    "keywords": [
        "yii2",
        "extension",
        "grid"
    ],
    "type": "yii2-extension",
    "license": "Apache-2.0",
    "homepage": "https://www.futuretek.cz/",
    "authors": [
        {
            "name": "Petr Leo Compel",
            "email": "petr.compel@futuretek.cz"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "^2.0.0",
        "kartik-v/yii2-grid": "~3.1",
        "himiklab/yii2-sortable-grid-view-widget": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "futuretek\\grid\\": ""
        }
    },
    "extra": {
        "yii-config": {
            "modules": {
                "gridview": {
                    "class": "\\kartik\\grid\\Module"
                }
            }
        }
    }
}
